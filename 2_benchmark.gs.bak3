function processHandles() {
  try {
    // ã‚¢ã‚¯ãƒ†ã‚£ãƒ–ãªã‚¹ãƒ—ãƒ¬ãƒƒãƒ‰ã‚·ãƒ¼ãƒˆã‚’å–å¾—
    var sheet = SpreadsheetApp.getActiveSpreadsheet().getActiveSheet();
