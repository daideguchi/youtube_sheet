      hasEditTrigger = true;
      break;
    }
  }

  // ãƒˆãƒªã‚¬ãƒ¼ãŒãªã‘ã‚Œã°ä½œæˆ
  if (!hasEditTrigger) {
    ScriptApp.newTrigger("onEdit").forSpreadsheet(ss).onEdit().create();
  }
